<h1>New Track</h1>

<!--<%= params[:project_id] %>-->

<%= form_for @track, :url => { :action => "create" } do |f| %>
  <%= f.text_field :title %>
  <%= f.hidden_field :project_id %>
  <%= f.submit "Create" %>
<% end %>

<a href="#" onclick="startrecording()">Start Recording</a>
<a href="#" onclick="stoprecording()">Stop Recording</a>